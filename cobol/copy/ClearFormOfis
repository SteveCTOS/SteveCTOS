      *
       CLEAR-SCREEN-FORM SECTION.
       CLSC-000.
             MOVE " " TO OFIS-NAME
                         OFIS-FIRSTNAME
                         OFIS-TITLE
                         OFIS-AFFILIATION
                         OFIS-CATEGORY
                         OFIS-PHONE
                         OFIS-PHONE-LABEL
                         OFIS-ADDRESS1
                         OFIS-ADDRESS2
                         OFIS-ADDRESS3
                         OFIS-FAX
                         OFIS-FAX-LABEL
                         OFIS-ADDRESS4
                         OFIS-ADDRESS5
                         OFIS-ADDRESS6.
             MOVE 1 TO SUB-40.
       CLSC-005.
             MOVE " " TO OFIS-LINE-DESC (SUB-40).
             IF SUB-40 < 200
                ADD 1 TO SUB-40
                GO TO CLSC-005.
       CLSC-010.
      *     UNLOCK OFIS-FILE.
       CLSC-999.
           EXIT.
