           SELECT STOCK-ALPHA-MASTER ASSIGN TO Ws-Stock
               ORGANIZATION IS INDEXED
               LOCK MANUAL
               ACCESS MODE IS DYNAMIC
               FILE STATUS IS WS-STOCK-STATUS
               RECORD KEY IS ST-AL-KEY
               ALTERNATE RECORD KEY IS ST-AL-ALT-KEY WITH DUPLICATES
               ALTERNATE RECORD KEY IS ST-AL-SUPPLIER WITH DUPLICATES.
