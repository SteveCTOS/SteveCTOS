           SELECT STOCK2-TRANS-FILE ASSIGN TO Ws-StTrans
               ORGANIZATION IS INDEXED
               LOCK MANUAL
               ACCESS MODE IS DYNAMIC
               RECORD KEY IS STTR2-KEY
               ALTERNATE RECORD KEY IS STTR2-ST-KEY WITH DUPLICATES
               ALTERNATE RECORD KEY IS STTR2-AC-KEY WITH DUPLICATES
               ALTERNATE RECORD KEY IS STTR2-DATE WITH DUPLICATES
               ALTERNATE RECORD KEY IS STTR2-COMPLETE WITH DUPLICATES
               ALTERNATE RECORD KEY IS 
                                   STTR2-STOCK-NUMBER WITH DUPLICATES
               ALTERNATE RECORD KEY IS 
                                 STTR2-ACCOUNT-NUMBER WITH DUPLICATES
               FILE STATUS IS WS-STTRANS2-STATUS.
