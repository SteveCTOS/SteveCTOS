fohtotext.py -r invoice -T $1.temp0.txt InPrintCo01
./text2pdf $1.temp0.txt -fCourier-Bold -t8 -s10 -x842 -y595 -c135 -l48 > $1.temp1.pdf
pdftk $1.temp1.pdf background invoice01.pdf output $1.temp2.pdf
pdftk $1.temp2.pdf cat 1-endwest output $1InPrintCo01.pdf
